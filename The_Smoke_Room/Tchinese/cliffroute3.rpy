# game/cliffroute3.rpy:552
translate Tchinese aftertent_9618df01:

    # cl "“There is? Splendid!”"
    cl "“有客棧？太好了！”"

# game/cliffroute3.rpy:553
translate Tchinese aftertent_0ee1ee8e:

